{
  "name": "@ableron/fastify",
  "version": "2.0.1",
  "description": "Ableron Server Side UI Composition - Fastify Plugin",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -fr dist/*",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types && sh ./tsconfig/set-package-type",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig/tsconfig-cjs.json",
    "build:types": "tsc -p tsconfig/tsconfig-types.json",
    "prettier": "prettier . --write",
    "test": "vitest --watch=false"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ableron",
    "fastify",
    "ssi",
    "server-side-includes",
    "ui-composition",
    "dynamic-ui-composition"
  ],
  "author": "Torsten Blasche <dev@t-blasche.de>",
  "license": "MIT",
  "homepage": "https://ableron.github.io/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ableron/ableron.git"
  },
  "bugs": {
    "url": "https://github.com/ableron/ableron/issues"
  },
  "dependencies": {
    "@ableron/ableron": "^2.0.0",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "fastify": "5.2.1",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "ts-loader": "9.5.2",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "prettier": {
    "printWidth": 120,
    "quoteProps": "consistent",
    "singleQuote": true,
    "trailingComma": "none"
  }
}
